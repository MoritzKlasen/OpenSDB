[req]
default_bits = 4096
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C = AT
ST = Vorarlberg
L = Dornbirn
O = OpenSDB
CN = admin.opensdb.local

[v3_req]
subjectAltName = @alt_names
basicConstraints = CA:false
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth

[alt_names]
DNS.1 = admin.opensdb.local
IP.1  = 10.115.1.170